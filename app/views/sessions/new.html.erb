<p id="notice"><%= notice %></p>
<h1>ログイン画面</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br>
  <p><%= f.label :password %></p>
  <%= f.password_field :password %>
  <br>
  <br>
  <%= f.submit "ログイン" %>
<% end %>
or
<%= link_to "登録する", new_user_path%>
